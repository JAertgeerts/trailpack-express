{
  "name": "trailpack-express",
  "version": "2.0.0",
  "description": "Express.js Trailpack. This pack binds the routes compiled in trailpack-router to a Express 4 or 5 Server.",
  "main": "index.js",
  "scripts": {
    "test": "eslint . && mocha && npm run testExpress4 && npm run testExpress5",
    "testExpress4": "export EXPRESS_VERSION=latest;mocha",
    "testExpress5": "export EXPRESS_VERSION=5.0.0-alpha.2;mocha",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "pre-commit": [
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaumard/trailpack-express.git"
  },
  "keywords": [
    "trails",
    "express",
    "express4",
    "express5",
    "trailsjs",
    "expressjs",
    "webserver",
    "trailpack"
  ],
  "author": "Jimmy Aumard <jimmy.aumard@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaumard/trailpack-express/issues"
  },
  "homepage": "https://github.com/jaumard/trailpack-express#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "consolidate": "^0.14.5",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.1",
    "express-boom": "^1.0.0",
    "express-session": "^1.14.2",
    "joi": "^10.0.2",
    "lodash": "^4.17.2",
    "method-override": "^2.3.6",
    "methods": "^1.1.2",
    "trailpack-webserver": "^2.0.0",
    "snyk": "^1.316.1"
  },
  "eslintConfig": {
    "extends": "trails"
  },
  "devDependencies": {
    "boom": "^4.2.0",
    "eslint": "^3.11.1",
    "pre-commit": "^1.1.3",
    "eslint-config-trails": "^2",
    "express": "^4.14.0",
    "mocha": "^3.2.0",
    "npm": "^3.10.10",
    "pug": "^2.0.0-beta6",
    "smokesignals": "^2.1.1",
    "supertest": "^2.0.1",
    "trailpack-footprints": "^2.0.0",
    "trailpack-router": "^2.1.0",
    "trails": "^2.0.0",
    "sqlite3": "^3.1.8",
    "trailpack-sequelize": "^2.0.0"
